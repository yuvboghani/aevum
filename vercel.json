{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/login",
            "destination": "/api/index.py"
        },
        {
            "source": "/callback",
            "destination": "/api/index.py"
        },
        {
            "source": "/busy",
            "destination": "/api/index.py"
        },
        {
            "source": "/tasks(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/calendars",
            "destination": "/api/index.py"
        },
        {
            "source": "/optimize(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/aevum/assets/(.*)",
            "destination": "/assets/$1"
        },
        {
            "source": "/aevum/(.*)",
            "destination": "/index.html"
        },
        {
            "source": "/aevum",
            "destination": "/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}